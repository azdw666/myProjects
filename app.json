      {
        "pagePath": "pages/living/living",
        "text": "首页"
      },
      {
        "pagePath": "pages/list/list",
        "text": "列表"
      },{
        "pagePath": "pages/index/index",
        "text":"个人中心"
      }
    ]
  },
  "sitemapLocation": "sitemap.json",
  "usingComponents": {
    "my-test":"/components/test1/test1",
    "my-rgba":"/components/Rgba/Rgba",
    "van-button": "vant-weapp/button",
